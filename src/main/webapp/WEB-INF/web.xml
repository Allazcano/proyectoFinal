<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UsuariosControlador</servlet-name>
        <servlet-class>controlador.UsuariosControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioControlador</servlet-name>
        <servlet-class>controlador.ServicioControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactoControlador</servlet-name>
        <servlet-class>controlador.ContactoControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilControlador</servlet-name>
        <servlet-class>controlador.PerfilControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuariosControlador</servlet-name>
        <url-pattern>/UsuariosControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioControlador</servlet-name>
        <url-pattern>/ServicioControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactoControlador</servlet-name>
        <url-pattern>/ContactoControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilControlador</servlet-name>
        <url-pattern>/PerfilControlador</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
